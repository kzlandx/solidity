pragma solidity ^0.8.24;

import {Test, console} from "forge-std/Test.sol";
import {SkullMemeNFT} from "src/SkullMemeNFT.sol";

contract MoodNftTest is Test {
    SkullMemeNFT skullMemeNFT;
    address USER = makeAddr("user");

    string public constant BERSERK_SVG_URI = "data:image/svg+xml;base64,";
    string public constant MINECRAFT_SVG_URI = "data:image/svg+xml;base64,";
    string public constant AOT_URI = "data:image/svg+xml;base64,";

    modifier minted {
        vm.prank(USER);
        skullMemeNFT.mintNft();
        _;
    }

    function setUp() public {
        skullMemeNFT = new SkullMemeNFT(BERSERK_SVG_URI, MINECRAFT_SVG_URI, AOT_URI);
    }

    function testViewTokenURI() public minted {
        console.log(skullMemeNFT.tokenURI(0));
    }

    function testTokenOwnerCanFlipMood() public minted {
        console.log("Initial mood: ", uint256(skullMemeNFT.getCurrentSkull(0)));
        vm.prank(USER);
        skullMemeNFT.flipCurrentSkull(0);
        console.log("Skull after flipping: ", uint256(skullMemeNFT.getCurrentSkull(0)));
        vm.prank(USER);
        skullMemeNFT.flipCurrentSkull(0);
        console.log("Skull after flipping again: ", uint256(skullMemeNFT.getCurrentSkull(0)));
        vm.prank(USER);
        skullMemeNFT.flipCurrentSkull(0);
        console.log("Skull after flipping yet again: ", uint256(skullMemeNFT.getCurrentSkull(0)));
    }

    function testTokenApprovedCanFlipMood() public minted {
        console.log("Initial skull: ", uint256(skullMemeNFT.getCurrentSkull(0)));
        address approvedUser = address(1);
        vm.prank(USER);
        skullMemeNFT.approveUser(approvedUser, 0);
        vm.prank(approvedUser);
        skullMemeNFT.flipCurrentSkull(0);
        console.log("Skull after flipping: ", uint256(skullMemeNFT.getCurrentSkull(0)));
    }
}